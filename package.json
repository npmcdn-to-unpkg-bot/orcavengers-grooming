  "description": "",
  "scripts": {
    "start": "npm run build && forever start server/index.js",
    "stop": "forever stop server/index.js",
    "build": "browserify -t reactify -t lessify ./client/index.jsx | uglifyjs > ./public/index.js",
    "watch": "nodemon server/index.js & npm run watch:client",
    "watch:client": "watchify -t reactify -t lessify ./client/index.jsx --debug -o ./public/index.js"
  },
