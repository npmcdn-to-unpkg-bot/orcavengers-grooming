    "nodemon": "^1.8.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "reactify": "^1.1.1",
    "socket.io-client": "^1.4.5",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "flux": "^2.1.1",
    "forever": "^0.15.1",
